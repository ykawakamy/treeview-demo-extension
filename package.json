{
  "name": "vscode-webview-treeview",
  "displayName": "vscode-webview-treeview",
  "description": "treeview in webview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "treeviewDemo",
          "name": "treeviewDemo"
        },
        {
          "id": "treeviewDemo1",
          "name": "treeviewDemo1"
        }
      ]
    },
    "commands": [
      {
        "command": "querySearch.replaceFiles",
        "title": "%replaceFiles%",
        "icon": "$(search-replace-all)"
      },
      {
        "command": "vscode-webview-treeview.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-webview-treeview.refresh",
        "title": "Hello refresh",
        "icon": "$(search-replace-all)"
      },
      {
        "command": "vscode-webview-treeview.reflesh2",
        "title": "Hello reflesh2",
        "icon": "$(search-replace-all)"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "querySearch.openFile",
          "when": "view == treeviewDemo && viewItem == querySearch.result && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.replace",
          "when": "view == treeviewDemo && viewItem == querySearch.result && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.openFile",
          "when": "view == treeviewDemo && viewItem == querySearch.file && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.replaceAll",
          "when": "view == treeviewDemo && viewItem == querySearch.file && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.copyResult",
          "when": "view == treeviewDemo"
        }
      ],
      "view/title": [
        {
          "command": "querySearch.replaceFiles",
          "when": "view == treeviewDemo1",
          "group": "navigation"
        },
        {
          "command": "vscode-webview-treeview.refresh",
          "when": "view == treeviewDemo",
          "group": "navigation"
        },
        {
          "command": "vscode-webview-treeview.refresh2",
          "when": "view == treeviewDemo1",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "dev-server": "^0.2.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-mock-vscode": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@react-hook/event": "^1.2.6",
    "@types/vscode-webview": "^1.57.5",
    "@vscode/codicons": "^0.0.36",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
